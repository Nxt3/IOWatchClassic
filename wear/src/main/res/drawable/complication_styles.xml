<?xml version="1.0" encoding="utf-8"?>
<android.support.wearable.complications.rendering.ComplicationDrawable
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:borderRadius="50dp"
    app:borderStyle="solid"
    app:borderWidth="1dp"
    app:highlightColor="@color/light_blue"
    app:iconColor="@color/white"
    app:rangedValuePrimaryColor="@color/teal"
    app:rangedValueRingWidth="1dp"
    app:rangedValueSecondaryColor="@color/white"
    app:textColor="@color/white"
    app:titleColor="@color/gray"
    app:textSize="14sp"
    app:titleSize="14sp">

    <ambient
        app:borderColor="@color/white"
        app:highlightColor="@color/white"
        app:iconColor="@color/white"
        app:rangedValuePrimaryColor="@color/white"
        app:titleColor="@color/white"
        app:textColor="@color/white"
        app:rangedValueSecondaryColor="@android:color/transparent"
        app:textTypeface="sans-serif-light"
        app:titleTypeface="sans-serif-light"/>
</android.support.wearable.complications.rendering.ComplicationDrawable>